{
  "name": "@applitools/eyes-universal",
  "version": "2.5.15",
  "description": "Applitools Eyes Universal SDK",
  "keywords": [
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests"
  ],
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:applitools/eyes.sdk.javascript1.git",
    "directory": "packages/eyes-universal"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "main": "./dist/index.js",
  "bin": {
    "eyes-universal": "./dist/cli.js"
  },
  "directories": {
    "lib": "./src",
    "example": "./example",
    "test": "./test"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint '**/*.ts'",
    "build": "yarn build:dist && yarn build:bin",
    "build:dist": "tsc",
    "build:bin": "pkg .",
    "build:zip": "zip -j ./bin/eyes-universal.zip ./bin/eyes-universal-linux ./bin/eyes-universal-macos ./bin/eyes-universal-win.exe",
    "build:tgz": "tar -czf ./bin/eyes-universal.tar.gz -C ./bin ./eyes-universal-linux ./eyes-universal-macos ./eyes-universal-win.exe",
    "test": "yarn test:it && yarn test:e2e",
    "test:unit": "mocha ./test/unit/*.spec.ts --exit --no-timeouts -r ts-node/register",
    "test:it": "echo 'mocha ./test/it/*.spec.js --no-timeouts'",
    "test:e2e": "mocha ./test/e2e/*.spec.ts --exit --no-timeouts -r ts-node/register",
    "deps": "bongo deps",
    "gh:publish": "gh workflow run publish-universal.yml --ref $(git rev-parse --abbrev-ref HEAD)",
    "preversion": "bongo preversion --verifyPendingChanges && yarn build && yarn build:zip && yarn build:tgz",
    "version": "yarn build && bongo version --withPendingChanges",
    "postversion": "bongo postversion"
  },
  "dependencies": {
    "@applitools/execution-grid-client": "1.1.13",
    "@applitools/eyes-sdk-core": "13.6.16",
    "@applitools/logger": "1.1.5",
    "@applitools/utils": "1.3.0",
    "@applitools/visual-grid-client": "15.12.28",
    "webdriver": "7.16.11",
    "ws": "7.4.6",
    "yargs": "17.4.1"
  },
  "devDependencies": {
    "@applitools/bongo": "^2.1.0",
    "@applitools/sdk-coverage-tests": "^2.3.18",
    "@applitools/test-utils": "^1.3.2",
    "@applitools/types": "1.4.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "12",
    "@types/ws": "^7.4.4",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^7.0.4",
    "mocha": "^9.0.0",
    "pkg": "^5.6.0",
    "prettier": "^2.1.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=12.x"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn bongo lint"
    }
  },
  "pkg": {
    "scripts": "./dist/cli.js",
    "outputPath": "./bin",
    "assets": [
      "./node_modules/vm2/lib/setup-sandbox.js"
    ],
    "targets": [
      "node14-linux-x64",
      "node14-macos-x64",
      "node14-win-x64"
    ],
    "compress": "GZip"
  }
}
